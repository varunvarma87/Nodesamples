{
    "port": 8011,
    "viewEngine": {
        "ext": "dust",
        "templatePath": ["public", "templates"]
    },

    "services": {
        "servicecore": {
            "scheme": "https:",
            "hostname": "live.qa.paypal.com",
            "rejectUnauthorized": false,
            "keepAlive": false,
            "socketTimeout": 900,
            "ciphers": "AES128+SHA1:3DES+SHA1",
            "secureProtocol": "TLSv1_method"
        }
    },

    "logger": {
        "type": "cal"
    }
}