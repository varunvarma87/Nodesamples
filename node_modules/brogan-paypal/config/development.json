{
    "services": {
        "servicecore": {
            "rejectUnauthorized": false
        }
    },
    "middleware": {
        "cal": {
            "module": {
                "name": "cal/middleware",
                "arguments": [{
                    "enableBuffering": false
                }]
            }
        },
        // Override logger options
        "pplogger": {
            "module": {
                "arguments": [{
                    "type": "console",
                    "format": "pretty"
                }]
            }
        }
    }
}